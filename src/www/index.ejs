<!doctype html>
<html lang="en" data-framework="react">

<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta http-equiv="expires" content="0">
    <meta http-equiv="cache-control" content="max-age=0"/>
    <meta http-equiv="cache-control" content="no-cache"/>
    <meta http-equiv="expires" content="0"/>
    <meta http-equiv="expires" content="Tue, 01 Jan 1980 1:00:00 GMT"/>
    <meta http-equiv="pragma" content="no-cache"/>
    <title>CassioPOS</title>
    <!-- Tell the browser to be responsive to screen width -->
    <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">

    <!-- Ionicons -->
    <link rel="stylesheet" href="repo-css/ionicons/2.0.1/css/ionicons.min.css">
    <!-- Font awesome -->
    <link rel="stylesheet" href="repo-css/font-awesome/4.7.0/css/font-awesome.min.css">

    <link href="repo-css/fonts-googleapis/fonts-googleapis.css"
          rel="stylesheet">

    <link rel="apple-touch-icon" sizes="57x57" href="img/favicon/apple-touch-icon-57x57.png">
    <link rel="apple-touch-icon" sizes="60x60" href="img/favicon/apple-touch-icon-60x60.png">
    <link rel="apple-touch-icon" sizes="72x72" href="img/favicon/apple-touch-icon-72x72.png">
    <link rel="apple-touch-icon" sizes="76x76" href="img/favicon/apple-touch-icon-76x76.png">
    <link rel="mask-icon" href="img/favicon/safari-pinned-tab.svg" color="blue">

    <link rel="icon" type="image/png" href="img/favicon/favicon-16x16.png" sizes="16x16">
    <link rel="icon" type="image/png" href="img/favicon/favicon-32x32.png" sizes="32x32">
    <link rel="icon" type="image/png" href="img/favicon/favicon-96x96.png" sizes="96x96">

    <style text="type/css">
        BODY, .app-content, .boot-container {
            margin: 0;
            border: 0;
            padding: 0;
        }

        .boot-container {
            display: flex;
            display: -webkit-flex;
            position: absolute;

            flex-direction: column;
            -webkit-flex-direction: column;
            align-items: center;
            -webkit-align-items: center;
            justify-content: space-around;
            -webkit-justify-content: space-around;
            width: 100%;
            height: 100%;
        }

        .boot-loader {
            display: flex;
            display: -webkit-flex;
            position: relative;

            flex-direction: column;
            -webkit-flex-direction: column;
            align-items: center;
            -webkit-align-items: center;
            justify-content: space-around;
            -webkit-justify-content: space-around;
            width: 210px;
            height: 210px;
        }

        @media screen and (-ms-high-contrast: active), (-ms-high-contrast: none) {
            .boot-loader {
                display: table;
            }

            .boot-message {
                display: table-cell;
                text-align: center;
                vertical-align: middle;
            }
        }

        .boot-message {
            font-family: "Source Sans Pro";
            font-smoothing: antialiased;
            color: #888;
        }

        .boot-circle1 {
            left: 10px;
            top: 10px;
            width: 190px;
            height: 190px;
            animation: boot-rotator 2s linear infinite;
        }

        .boot-circle2 {
            left: 20px;
            top: 20px;
            width: 170px;
            height: 170px;
            animation: boot-rotator 1.5s linear infinite;
        }

        .boot-circle3 {
            left: 30px;
            top: 30px;
            width: 150px;
            height: 150px;
            animation: boot-rotator 1s linear infinite;
        }

        .boot-circle {
            position: absolute;
        }

        .boot-circle circle {
            stroke: #888;
            stroke-width: 5;
            stroke-opacity: 0.1;
            fill: transparent;
        }

        .boot-circle path {
            stroke: #D9202C;
            stroke-width: 5;
            stroke-opacity: 1;
            fill: transparent;
            stroke-linecap: round;
            stroke-linejoin: round;
        }

        .boot-circle2 path {
            stroke: #DD5624;
        }

        .boot-circle3 path {
            stroke: #F9A533;
        }

        @keyframes boot-rotator {
            0% {
                transform: rotate(0deg);
                -webkit-transform: rotate(0deg);
            }
            100% {
                transform: rotate(360deg);
                -webkit-transform: rotate(360deg);
            }
        }
    </style>

    <!-- No auto translation from google -->
    <meta name="google" content="notranslate"/>

</head>

<body class="sidebar-mini">

<div class="boot-container remove-after-boot">
    <div class="boot-loader">
        <div class="boot-message">Sopra Banking</div>
        <svg class="boot-circle boot-circle1" viewBox="0 0 220 220">
            <circle cx="110" cy="110" r="100"/>
            <path d="M110,10 A100,100,0,0,1,180,39"/>
        </svg>
        <svg class="boot-circle boot-circle2" viewBox="0 0 220 220">
            <circle cx="110" cy="110" r="100"/>
            <path d="M110,10 A100,100,0,0,1,180,39"/>
        </svg>
        <svg class="boot-circle boot-circle3" viewBox="0 0 220 220">
            <circle cx="110" cy="110" r="100"/>
            <path d="M110,10 A100,100,0,0,1,180,39"/>
        </svg>
    </div>
</div>


<section id="app-content">
</section>
<!-- React generated script -->
<script src="config.js"></script>
<script src="i18n/localeCorrespondances.js"></script>

<%= htmlWebpackPlugin.files.webpackManifest %>
</body>

</html>
