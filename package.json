{
  "name": "accelerator",
  "private": true,
  "scripts": {
    "start": "cross-env NODE_ENV=dev webpack-dev-server --progress --config webpack.config.dev.js",
    "build": "webpack -p --progress --config webpack.config.build.js",
    "build:langs": "./node_modules/.bin/babel-node intl/translate.js",
    "build-cordova": "webpack --config webpack.config.cordova.js  --progress --colors",
    "tool:lang-merge": "./node_modules/.bin/babel-node intl/merge.js",
    "test-browser-window": "start http://localhost:1337/",
    "test-browser-unix": "xdg-open http://localhost:1337/",
    "test-browser-mac": "open http://localhost:1337/",
    "test-server": "webpack-dev-server --config webpack-test-server.config.js --progress --inline --colors",
    "test": "npm run test-browser-window && npm run test-server",
    "test-u": "npm run test-browser-unix && npm run test-server",
    "test-m": "npm run test-browser-mac && npm run test-server"
  },
